        ".": {
            "import": "./dist/revert-react.es.js",
            "require": "./dist/revert-react.umd.js"
        }
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "@scarf/scarf": "^1.3.0"
    }
